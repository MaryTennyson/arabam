<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navi_graph"
    app:startDestination="@id/listingPageFragment">

    <fragment
        android:id="@+id/listingPageFragment"
        android:name="com.arabam.android.view.ListingPageFragment"
        android:label="fragment_listing_page"
        tools:layout="@layout/fragment_listing_page" >
        <action
            android:id="@+id/action_listingPageFragment_to_detailsPageFragment"
            app:destination="@id/detailsPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsPageFragment"
        android:name="com.arabam.android.view.DetailsPageFragment"
        android:label="DetailsPageFragment" >
        <action
            android:id="@+id/action_detailsPageFragment_to_listingPageFragment"
            app:destination="@id/listingPageFragment" />
    </fragment>
</navigation>